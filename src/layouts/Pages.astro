---
import Base from "./Base.astro";
interface Props {
	title: string;
}

const { title } = Astro.props;
---

<Base
  title={`${title} | Micah Asowata`}
  description="Micah Asowata is a software engineer that enjoys building products of all kinds with small, very focused teams."
  img={{
    src: "https://utfs.io/f/dacc1f27-316e-41c5-9e07-064369446e66-2uz.jpg",
    alt: "micahasowata.com logo",
  }}
  ,
  type="website"
>
  <slot />
</Base>
